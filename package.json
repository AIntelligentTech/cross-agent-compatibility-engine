{
  "name": "cace-cli",
  "version": "2.2.0",
  "description": "Cross-Agent Compatibility Engine - Convert and validate AI agent components between Claude, Cursor, Windsurf, OpenCode, Codex, and Gemini",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cace": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build && bun run typecheck && bun test"
  },
  "keywords": [
    "ai-agents",
    "claude-code",
    "windsurf",
    "cursor",
    "gemini-cli",
    "opencode",
    "codex",
    "openai-codex",
    "aider",
    "workflow",
    "skill",
    "command",
    "conversion",
    "compatibility",
    "migration",
    "llm",
    "ai-tools"
  ],
  "author": "AIntelligent Tech <hello@aintelligenttech.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIntelligentTech/cross-agent-compatibility-engine.git"
  },
  "bugs": {
    "url": "https://github.com/AIntelligentTech/cross-agent-compatibility-engine/issues"
  },
  "homepage": "https://github.com/AIntelligentTech/cross-agent-compatibility-engine#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bun": "^1.1.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "gray-matter": "^4.0.3",
    "chalk": "^5.3.0",
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
